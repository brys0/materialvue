{
  "name": "@cosmicmind/materialjs",
  "version": "0.0.1-rc-120922-1",
  "description": "A Material Design framework in TypeScript.",
  "keywords": [],
  "author": {
    "name": "Daniel Jonathan",
    "email": "daniel@cosmicverse.org"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmicmind/materialjs.git"
  },
  "type": "module",
  "main": "dist/lib.es.js",
  "module": "dist/lib.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "typings",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "lint": "eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "build": "run-s build:lib build:app build:server",
    "build:lib": "vite build --config vite.config.ts --mode production",
    "build:app": "vite build --config vite.config.app.ts --mode production",
    "build:server": "vite build --config vite.config.server.ts --mode production",
    "watch": "run-p watch:lib watch:app watch:server",
    "watch:lib": "vite build --watch --config vite.config.ts --mode development",
    "watch:app": "vite build --watch --config vite.config.app.ts --mode development",
    "watch:server": "vite build --watch --config vite.config.server.ts --mode development",
    "nodemon": "nodemon dist/server.js",
    "dev": "run-p watch nodemon",
    "start": "node dist/server.js",
    "app": "run-s build start",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@cosmicmind/foundation": "^0.0.1-rc-120822-2",
    "@koa/router": "^12.0.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "keygrip": "^1.1.0",
    "koa": "^2.13.4",
    "koa-body": "^6.0.1",
    "koa-static": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.0.21",
    "sass": "^1.55.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/koa": "^2.13.5",
    "@types/koa__router": "^12.0.0",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitest/coverage-c8": "^0.23.4",
    "@vue/compiler-sfc": "^3.2.38",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.5.1",
    "jsdom": "^20.0.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "vitest": "^0.23.0"
  }
}
